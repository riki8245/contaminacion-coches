#BUILD
docker build -t rikisimo/contaminacioncochescastellon:latest .

#TAG
#Se pone 2 veces si es para sobreescribir
docker tag rikisimo/contaminacioncochescastellon:latest rikisimo/contaminacioncochescastellon:latest

#Sino
docker tag rikisimo/contaminacioncochescastellon:latest

#Y Push
docker push rikisimo/contaminacioncochescastellon:latest

#Ejecutar po comandos
docker run -p 8080:80 rikisimo/contaminacioncochescastellon:latest
